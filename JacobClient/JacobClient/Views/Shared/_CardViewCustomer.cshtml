@using JacobClient.Models
@model Customer
<div id="tableCustomer" class="well">
    <div class="row">
        <h3><strong>Customer</strong></h3>
    </div>
    <fieldset @Html.Raw(Model.IsControlEnabled ? "" : "disabled")>
        <div class="row">
            <div class="col-md-3">
                <div class="col-md-4">
                    @Html.LabelFor(m => m.Id, new { @class = "" })
                </div>
                <div class="col-md-8">
                    @Html.TextBoxFor(m => m.Id, new { @class = "form-control" })
                </div>
            </div>
            <div class="col-md-3">
                <div class="col-md-4">
                    @Html.LabelFor(m => m.FirstName, new { @class = "" })
                </div>
                <div class="col-md-8">
                    @Html.TextBoxFor(m => m.FirstName, new { @class = "form-control" })
                </div>
            </div>
            <div class="col-md-3">
                <div class="col-md-4">
                    @Html.LabelFor(m => m.LastName, new { @class = "" })
                </div>
                <div class="col-md-8">
                    @Html.TextBoxFor(m => m.LastName, new { @class = "form-control" })
                </div>
            </div>
            <div class="col-md-3">
                <div class="col-md-4">
                    @Html.LabelFor(m => m.PhoneNumber, "Phone", new { @class = "" })
                </div>
                <div class="col-md-8">
                    @Html.TextBoxFor(m => m.PhoneNumber, new { @class = "form-control" })
                </div>
            </div>
        </div>
    </fieldset>
</div>